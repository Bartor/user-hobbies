{
    "testRegex": "((\\.|/*.)(spec))\\.tsx?$",
    "transform": {
        "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
    ],
    "moduleNameMapper": {
        "\\.(css|scss|less|sass)$": "identity-obj-proxy"
    }
}